{
  "suggest.noselect": false,
  "coc.preferences.extensionUpdateCheck": "daily",
  "languageserver": {
    "golang": {
      "command": "gopls",
      "rootPatterns": ["go.mod", ".vim/", ".git/", ".hg/"],
      "filetypes": ["go"]
    },
    "elixir": {
      "command": "~/.elixir-ls/release/language_server.sh",
      "filetypes": ["elixir", "eelixir"]
    }
  }
}
